{% set html %}{% include '@!NelmioApiDoc/SwaggerUi/index.html.twig' %}{% endset %}

{% set style %}
    {% block style %}
        <style>
            body {
                margin: 60px 0;
            }

            header::before {
                background: #24272A;
                box-shadow: none;
                height: 60px;
            }

            header #logo {
                background-image: url('data:image/png;base64,');
                background-size: cover;
                display: block;
                top: 28px;
                width: 90px; height: 30px;
            }

            #logo img {
                display: none;
            }

            #swagger-ui.api-platform .information-container.wrapper {
                padding: 10px 0;
            }

            .swagger-ui .info hgroup.main {
                margin: 0;
            }

            #swagger-ui.api-platform .info .title {
                color: #3B4151;
            }

            .swagger-ui .info .description:empty {
                display: none;
            }

            #swagger-ui.api-platform .opblock-tag:hover {
                background-color: transparent;
                transform: none;
            }

            .swagger-ui .expand-operation {
                margin-top: 7px;
            }

            #swagger-ui + .swagger-ui-wrap {
                display: none;
            }
        </style>
    {% endblock %}
{% endset %}

{{ html|replace({'</head>': "#{style}</head>", 'https://github.com/nelmio/NelmioApiDocBundle': 'http://www.vanio.cz/'})|raw }}
